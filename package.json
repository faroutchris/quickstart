{
  "name": "@reddigital/quickstart",
  "description": "Frontend island architecture for Adonisjs",
  "version": "0.0.19",
  "engines": {
    "node": ">=20.6.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": "./build/index.js",
    "./provider": "./build/providers/quick_provider.js",
    "./plugins/vite": "./build/src/plugins/vite.js",
    "./types": "./build/src/types.js",
    "./init": "./build/src/web_component/init.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles --up 1 \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --write .",
    "quick:test": "node --enable-source-maps --import=ts-node-maintained/register/esm bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsup-node",
    "postbuild": "npm run copy:templates",
    "release": "release-it",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "6.17.1",
    "@adonisjs/eslint-config": "^2.1.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/tsconfig": "^1.4.1",
    "@adonisjs/vite": "^4.0.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "4.0.1",
    "@japa/expect-type": "^2.0.3",
    "@japa/file-system": "^2.3.2",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "4.2.0",
    "@japa/snapshot": "^2.0.8",
    "@release-it/conventional-changelog": "^10.0.1",
    "@swc/core": "1.10.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.1",
    "@types/qs": "^6.14.0",
    "@types/supertest": "^6.0.3",
    "@vavite/multibuild": "^5.1.0",
    "c8": "^10.1.3",
    "copyfiles": "^2.4.1",
    "del-cli": "^6.0.0",
    "edge-parser": "^9.0.4",
    "edge.js": "^6.2.1",
    "eslint": "^9.29.0",
    "get-port": "^7.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.3",
    "supertest": "^7.1.1",
    "ts-node-maintained": "^10.9.5",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@poppinss/utils": "^6.9.4",
    "@tuyau/utils": "^0.0.8",
    "edge-error": "^4.0.2",
    "html-entities": "^2.6.0",
    "locate-path": "^7.2.0",
    "qs": "^6.14.0"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.9.1",
    "@adonisjs/session": "^7.4.0",
    "@adonisjs/vite": "^4.0.0",
    "@japa/api-client": "^2.0.0 || ^3.0.0",
    "edge.js": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@japa/api-client": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Reddigital <null@dev.com>",
  "license": "MIT",
  "homepage": "https://github.com/faroutchris/quickstart-adonis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faroutchris/quickstart-adonis.git"
  },
  "bugs": {
    "url": "https://github.com/faroutchris/quickstart-adonis/issues"
  },
  "keywords": [
    "island architecture",
    "hydration",
    "svelte",
    "adonisjs"
  ],
  "prettier": "@adonisjs/prettier-config",
  "release-it": {
    "git": {
      "requireUpstream": true,
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "push": true,
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true,
      "skipChecks": true,
      "tag": "latest"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "tests_helpers/**"
    ]
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./providers/quick_provider.ts",
      "./src/types.ts",
      "./src/web_component/init.ts",
      "./src/plugins/vite.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": true,
    "target": "esnext"
  },
  "packageManager": "pnpm@10.12.1"
}
